{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "test",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview -- --env preview",
    "preview:n": "yarn build && next start",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "dev": "next dev --experimental-https --port 3333",
    "build:prod": "opennextjs-cloudflare build",
    "build": "next build"
  },
  "dependencies": {
    "next": "15.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "server-only": "0.0.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20250525.0",
    "@opennextjs/cloudflare": "1.3.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "@vanilla-extract/next-plugin": "^2.4.10",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "esbuild": "^0.25.0",
    "happy-dom": "^17.4.4",
    "typescript": "~5.7.0",
    "vercel": "^44.2.11",
    "wrangler": "^4.23.0"
  },
  "packageManager": "yarn@4.6.0"
}
